{
    "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "outDir": "./scripts/js",
      "rootDir": "./scripts/ts",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "sourceMap": true
    },
    "include": ["scripts/ts/**/*"],
    "exclude": ["node_modules"]
  }